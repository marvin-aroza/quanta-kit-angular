{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "quanta-kit": {
      "projectType": "library",
      "root": "projects/quanta-kit",
      "sourceRoot": "projects/quanta-kit/src",
      "prefix": "lib",
      "architect": {
        "build": {
          "builder": "@angular/build:ng-packagr",
          "configurations": {
            "production": {
              "tsConfig": "projects/quanta-kit/tsconfig.lib.prod.json"
            },
            "development": {
              "tsConfig": "projects/quanta-kit/tsconfig.lib.json"
            }
          },
          "defaultConfiguration": "production"
        },
        "test": {
          "builder": "@angular/build:karma",
          "options": {
            "tsConfig": "projects/quanta-kit/tsconfig.spec.json",
            "karmaConfig": "projects/quanta-kit/karma.conf.js",
            "main": "projects/quanta-kit/src/test.ts"
          }
        },
        "storybook": {
          "builder": "@storybook/angular:start-storybook",
          "options": {
            "configDir": "projects/quanta-kit/.storybook",
            "browserTarget": "quanta-kit:build",
            "compodoc": true,
            "compodocArgs": [
              "-e",
              "json",
              "-d",
              "projects/quanta-kit"
            ],
            "port": 6006
          }
        },
        "build-storybook": {
          "builder": "@storybook/angular:build-storybook",
          "options": {
            "configDir": "projects/quanta-kit/.storybook",
            "browserTarget": "quanta-kit:build",
            "compodoc": true,
            "compodocArgs": [
              "-e",
              "json",
              "-d",
              "projects/quanta-kit"
            ],
            "outputDir": "storybook-static"
          }
        }
      }
    }
  },
  "cli": {
    "analytics": "99c86055-7613-45d0-947b-de2f9b8f369b"
  }
}